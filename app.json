{
  "expo": {
    "name": "Purple-School-App",
    "slug": "Purple-School-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Purple-School-App",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/FiraSans-Regular.ttf"],
          "android": {
            "fonts": [
              {
                "fontFamily": "FiraSans",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/FiraSans-Thin.ttf",
                    "weight": 100
                  },
                  {
                    "path": "./assets/fonts/FiraSans-ThinItalic.ttf",
                    "weight": 100,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-ExtraLight.ttf",
                    "weight": 200
                  },
                  {
                    "path": "./assets/fonts/FiraSans-ExtraLightItalic.ttf",
                    "weight": 200,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./assets/fonts/FiraSans-LightItalic.ttf",
                    "weight": 300,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/FiraSans-Italic.ttf",
                    "weight": 400,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/FiraSans-MediumItalic.ttf",
                    "weight": 500,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./assets/fonts/FiraSans-SemiBoldItalic.ttf",
                    "weight": 600,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/FiraSans-BoldItalic.ttf",
                    "weight": 700,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-ExtraBold.ttf",
                    "weight": 800
                  },
                  {
                    "path": "./assets/fonts/FiraSans-ExtraBoldItalic.ttf",
                    "weight": 800,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/FiraSans-Black.ttf",
                    "weight": 900
                  },
                  {
                    "path": "./assets/fonts/FiraSans-BlackItalic.ttf",
                    "weight": 900,
                    "style": "italic"
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/FiraSans-Thin.ttf",
              "./assets/fonts/FiraSans-ThinItalic.ttf",
              "./assets/fonts/FiraSans-ExtraLight.ttf",
              "./assets/fonts/FiraSans-ExtraLightItalic.ttf",
              "./assets/fonts/FiraSans-Light.ttf",
              "./assets/fonts/FiraSans-LightItalic.ttf",
              "./assets/fonts/FiraSans-Regular.ttf",
              "./assets/fonts/FiraSans-Italic.ttf",
              "./assets/fonts/FiraSans-Medium.ttf",
              "./assets/fonts/FiraSans-MediumItalic.ttf",
              "./assets/fonts/FiraSans-SemiBold.ttf",
              "./assets/fonts/FiraSans-SemiBoldItalic.ttf",
              "./assets/fonts/FiraSans-Bold.ttf",
              "./assets/fonts/FiraSans-BoldItalic.ttf",
              "./assets/fonts/FiraSans-ExtraBold.ttf",
              "./assets/fonts/FiraSans-ExtraBoldItalic.ttf",
              "./assets/fonts/FiraSans-Black.ttf",
              "./assets/fonts/FiraSans-BlackItalic.ttf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
